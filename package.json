{
  "name": "nodify-web",
  "version": "0.1.0",
  "description": "nodify web site",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Hasan Oezdemir",
  "license": "MIT",
  "repository": {},
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-dom": "^16.0.5",
    "@types/react-icons": "^2.2.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-slick": "^0.23.0",
    "@types/reactstrap": "^5.0.26",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^23.0.0",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.1.5",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "availity-reactstrap-validation": "^2.0.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "bubbletree": "^2.0.2",
    "raphael": "^2.2.7",
    "react": "^16.4.0",
    "react-cookie-banner": "^4.0.0",
    "react-dom": "^16.4.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-slick": "^0.23.1",
    "react-waypoint": "^8.0.1",
